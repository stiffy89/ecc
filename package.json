{
  "name": "travelexp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^7",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
        "travelexpenses": {
            "kind": "odata",
            "model": "srv/external/travelexpenses",
            "[production]": {
                "credentials": {
                    "destination": "dest_ecc",
                    "path" : "/sap/opu/odata/sap/z_travel_and_expenses_cfg_srv"
                }
            }
        },
        "[production]": {
            "auth": {
                "kind": "xsuaa",
                "restrict_all_services": false
            }
        },
        "auth": "xsuaa"
    }
  }
}